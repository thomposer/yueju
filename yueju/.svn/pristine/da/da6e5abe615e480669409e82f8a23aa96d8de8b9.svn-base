/***************************图片部分******************/
#h_picture{
	width: 1170px;
	height: 1280px;
	/*overflow: hidden;*/
	/*background: blue;*/
	/*position: relative;*/
	position: relative;
}
.pic_d{
	width: 370px;
	height: 295px;
	float: left;
	border: solid 1px #C0C0C0;
	margin-bottom: 20px;
	border-radius: 2px;
}
.pic_i{
	width: 370px;
	height: 230px;
}
.h_r_title{
	position: absolute;
	top: 0px;
	color:#676d6d;
}
.h_r_city{
	position: absolute;
	top: 0px;
	color:#676d6d;
}
