	width: 1170px;
	height: 1800px;
	margin: 0 auto;
	/*background: black;*/
}
#foot-wrapper{
	width: 1170px;
	height: 170px;
	margin: 0 auto;
	/*background: red;*/
}
ul{
	list-style: none;
}
a{
	text-decoration: none;
}
/*************************************************整体基本布局结束********************************************************/
/**********************************header开始**********************/
#logo{
	margin-top: 20px;
}

#header-title{
	float: right;
}
#header-title li{
	margin-top: 20px;
	float: left;
	margin-right: 32px;
}
#header-title a{
	color: black;
}
/**********************header结束*************/
/**********************mian开始***********************/
#query{
	width: 960px;
	height: 50px;
	margin: 22px auto;
	background:white;
	border-radius: 5px;
}
#query-body{
	width: 292px;
	height: 48px;
	/*background: black;*/
	border: solid 1px #C0C0C0;
	border-right: 0px;
	border-radius:3px;
	float: left;
}
#select1{
	width: 145px;
	height: 50px;
	line-height: 50px;
	/*background: red;*/
	text-align: center;
	float: left;
	color: #C0C0C0;
	border-right:solid 1px #C0C0C0;
	cursor: pointer;
	/*position: absolute;*/
}
#select1 ul{
	position: relative;
	z-index: 500;
}
#select1 li{
	background: black;
	color: white;
	border-bottom: solid 1px white;
	display: none;
	opacity: 0.7;
	/*z-index: 5;*/
	/*position: absolute;*/
	/*clear:both;*/
}
#select2{
	width: 145px;
	height: 50px;
	line-height: 50px;
	/*background: red;*/
	color: #C0C0C0;
	text-align: center;
	float: left;
	cursor: pointer;
	/*border-right:solid 1px #C0C0C0;*/
}
#select2 ul{
	position: relative;
	z-index: 500;
}
#select2 li{
	background: black;
	color: white;
	border-bottom: solid 1px white;
	display: none;
	opacity: 0.7;
	z-index: 500;
}
#select-input{
	width:455px;
	height: 50px;
	color: #C0C0C0;
	border: solid 1px #C0C0C0;
	outline: none;
	font-size: 16px;
	padding-left: 10px;
	float: left;
}
#sou{
	width: 55px;
	height: 20px;
	border:0;
	float: left;
	background: #39c6a7;
	font-size: 16px;
	color: white;
	margin-left: 60px;
	margin-top: 15px;
	cursor:pointer;
	outline: none;
}
#sub{
	float: right;
	width: 210px;
	height: 50px;
	background:#39c6a7 ;
	text-align: center;
	cursor:pointer;
}
#sub img{
	margin-top: 20px;
}
