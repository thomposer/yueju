#header{
	width:100%;
	height: 60px;
	/*background: url("../img/house-color2.png");*/
	background: white;
	border-bottom: solid black 1px ;
}
#main{
	width: 100%;
	/*height: 1800px;*/
	background: url("../img/house-color2.png");
	overflow: hidden;
}
#foot{
	width: 100%;
	height: 170px;
	background:#152636;
}
#header-wrapper{
	width: 1170px;
	height: 100%;
	margin: 0 auto;
	/*background: red;*/
}
