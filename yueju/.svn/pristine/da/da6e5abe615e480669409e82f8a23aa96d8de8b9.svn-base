	margin-left: 10px;
	color:#b7bdbd;
	line-height: 30px;
}

/**********************foot******************/
#foot{
	overflow: hidden;
}
#f_logo{
	width: 270px;
	height: 120px;
	background: url("../img/f_logo.png") no-repeat;
	background-size: cover;
	margin-top: 40px;
	float: left;
}
#f_ma{
	width: 368px;
	height: 120px;
	background: url("../img/foot_ma.png") no-repeat;
	float: right;
	margin-top: 40px;
}
#f_biao{
	width: 300px;
	margin-top: 50px;
	float: left;
	height: 120px;
}
#f_biao li{
	margin-bottom: 5px;
	color: white;
}
#f_biao a{
	color: white;
}
#fenye{
	width: 800px;
	height: 30px;
	margin: 0 auto;
	/*background: blue;*/
}
#fenye a{
	width: 60px;
	height: 20px;
	float: left;
	border: solid 1px darkgray;
	line-height: 20px;
	text-align: center;
	color: black;
	margin-right: 20px;
	border-radius: 2px;
}
.house{
	background: #fff;
	margin-top:20px;
	height:285px;
	width: 370px;
	margin-left:30px;
	float: left;
}
#service a{
	color:#e0dfdf;
}
.house img{
	width: 370px;
	height: 225px;
	float: left;
}
.brief{
	height:60px;
	border: 1px solid #e0dfdf;    background: #fff;
	float: left;
	width: 370px;

}
.brief p{
	text-align: left;
	font-size:12px;
	color: #535858;
	margin:10px 0;
	width:370px;
	line-height:20px;
	height:20px;
	overflow: hidden;
	-ms-text-overflow: ellipsis;
	text-overflow: ellipsis;
	white-space:nowrap;
}
.brief p:first-child{
	font-size:14px;
	color: #213433;
	text-align: right;
	margin-bottom:0;
}
.home-title{
	display: inline-block;
	width:200px;
	text-overflow:ellipsis;
	overflow: hidden;
	float: left;
	text-align: left;
	white-space:nowrap;
}
#recommend h2{
	text-align: center;
	color: #2f403c;
	font-size:18px;
}
#recommend-more {
	margin-top: 10px;
	color: #2f403c;
	text-align: center;
}