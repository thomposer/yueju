/*created by zjl*/
html{
    width: 100%;
    height: 1063px;
    font-family: '微软雅黑';
}
body{
    width: 100%;
    height: 1063px;
    background-color: #eff3f5;
    /*overflow: hidden;*/
    clear: both;
    font-family: '微软雅黑';
    position: relative;
}
#header{
    height: 61px;
    background-color: #fff;
}
#content{
    overflow: hidden;
    clear: both;
    margin-top: 26px;
    position: relative;
}
#sidebar{
    width: 370px;
    height: 456px;
    background-color: #fff;
    overflow: hidden;
    float: left;
    position: relative;
}
#sidebar .menu{
    width: 114px;
    height: 363px;
    margin: 49px 138px 45px  120px;
}
#sidebar .list {

    height: 45px;
    line-height: 45px;
    margin-bottom: 16px;
}
#sidebar .list a{
    float: right;
    color: #6a6e71;
    font-size: 18px;
}
#sidebar .list a.active{
    color: #5ce1a8;
}
#sidebar .list .sidebar-icon{
    display: inline-block;
    width: 27px;
    height:26px;
    margin-top: 10px;
    /*line-height: 26px;*/
    float: left;

}
#sidebar .list .order{
    background: url("../img/spirts.png") no-repeat -27px -26px;
}
#sidebar .list .collect{
    background: url("../img/spirts.png") no-repeat -29px 0;
}
#sidebar .list .info{
    background: url("../img/spirts.png") no-repeat -54px -25px;
}
#sidebar .list .comment{
    background: url("../img/spirts.png") no-repeat 0px -26px;
}
#sidebar .list .profile{
    background: url("../img/spirts.png") no-repeat 0 0;
}
#sidebar .list .password{
    background: url("../img/spirts.png") no-repeat 0px -54px;
}
#sidebar .chosestyle{
    width:7px;
    height: 58px;
    background-color: #48b189;
    position:absolute;
    right: 0;
}
#main-con{
    width: 770px;
    float: right;
    overflow: hidden;
    /*position: relative;*/
}
#main-con  .comment-list{
    width: 770px;
    height: 262px;
    background-color: #fff;
    margin-bottom: 17px;
    overflow: hidden;
}
#main-con .com-content{
    width:695px;
    height:222px;
    margin: 21px auto;

}
#main-con .left{
    display: inline-block;
    width: 145px;
    height:130px;
    margin-top: 0;

}
#main-con .left .com-img{
    width: 145px;
    height: 75px;
    margin-bottom: 14px;
}
#main-con .left .img{
    display: inline-block;
    width: 145px;
    height: 75px;
    margin-bottom: 14px;
    background: url("../img/images.png") no-repeat -10px -20px;
}
#main-con .left .star-letter{
    text-align: center;
    color: #5b6661;
    font-size: 14px;
}
.star{
    width: 85px;
    height: 15px;
    margin: auto;
    padding-top: 10px;
    text-align: center;
    /*height: 30px;*/
}
.star img{
    width: 15px;
    height: 15px;
}
#main-con .right{
    display: inline-block;
    width:510px;
    height:222px;
    float: right;
}
 .com-desc{
    display:inline-block;
    width:510px;
    height:75px;
}
 .com-title{
    height:16px;
    line-height:16px;
    font-size: 16px;
    color:#39454b;
}
 .com-abstract{
    margin-top: 10px;
    height:14px;
    line-height:14px;
    font-size: 14px;
    color:#7d8387;
}
.com-time{
    margin-top: 9px;
    height:16px;
    line-height: 16px;
    font-size: 16px;
    color: #2eb57b;
    margin-bottom: 9px;
}
 #mask .com-time-tc{
     margin-top: 9px;
     height:16px;
     line-height: 16px;
     font-size: 16px;
     color: #2eb57b;
     margin-bottom: 9px;
 }
#main-con .right .com-detail{
    display: inline-block;
    width:510px;
    height: 131px;
    margin-top: 14px;
}
#main-con .right .com-letter{
    font-size: 16px;
    color: #39454b;
}
#main-con .right .com-span{
    display: inline;
    width: 510px;
    height: 59px;
    font-size: 14px;
    color:#a8acae;
    margin-top: 12px;
    line-height:23px;
}
#main-con .right .com-passage{
    width: 510px;
    height: 70px;
    font-size: 14px;
    color:#a8acae;
    margin-top: 12px;
    line-height:23px;
    overflow: hidden;
    text-overflow: ellipsis;
}
 .com-btngroup{
    width:266px;
    height:30px;
    float: right;
     margin-top: 5px;
}
 .com-btn{
    width:123px;
    height:29px;
    border:1px solid #5ce1a8;
    display: inline-block;
    text-align: center;
    color: #5ce1a8;
    line-height: 29px;
     font-size: 16px;
     background-color: #ffffff;

}
 .com-cancel ,.mask-com-cancel{
    float:left;
}
 .com-revise,.mask-com-revise{
    float: right;
}
  .com-btn:hover{
    background-color: #5ce1a8;
    color:#fff;
}
.page-num{

    margin-top: 15px;
    width: 770px;
    height: 30px;
    overflow: hidden;
    text-align: center;
}
.each-page{

    display: inline-block;
    margin:0 auto;
}
.each-page li{
    background-color: #ffffff;
    /*float: left;*/
    width: 27px;
    height: 27px;
    border: 1px solid #c4c4c4;
    line-height: 27px;
    text-align: center;
    margin-right: 11px;
    color: #888585;
    display: inline-block;
    position: relative;
}
.each-page li a{
    text-decoration: none;
    width: 27px;
    height: 27px;
    color: #888585;
}
.each-page .comment_selected {
    background-color: #5ce1a8;
}
.each-page .comment_selected a{
    color: #ffffff;
}
.each-page .big-page{
    width: 61px;
    height: 27px;
}
 #mask{

     display: none;
    width: 100%;
    height:100%;
    position: absolute;
    top: 0;
    left:0;
     right: 0;
     bottom:0;
    /*background-color: #b7bfc3;*/
    /*opacity: 0.7;*/
     background-color: rgba(195,200,202,.5);
}
 #mask .wrapper{
     height:322px;
     position: relative;
     top:120px;
 }
 .com-tanc{
    /*display: none;*/
    z-index: 1;
    position: absolute;
     right:0;
    /*top:88px;*/
     top:0;
    width:770px;
    height:322px;
    background-color: #ffffff;
}
 #mask .container{
     width: 696px;
     height: 289px;
     margin:20px 33px 11px 42px;

 }
 #mask .com-left-img{
     width:144px;
     height:77px;
     float: left;
     /*margin-right: 40px;*/
 }
 #mask .com-right{
     width: 512px;
     height:297px;
     float: right;
 }
#mask .com-right .com-starnum{
    margin-top: 15px;
    margin-bottom: 20px;
    font-size: 16px;
    height:16px;
    line-height:16px;
    color: #495650;
}
#mask .com-right .mycomment{
    color:#39454b;
    font-size: 16px;
    height:16px;
    line-height:16px;
    margin-bottom: 10px;
}
#mask .com-right .com-content{
    background-color: #f4f5f5;
    width: 512px;
    height: 94px;
    margin: 0;
    border:0;
}
#mask .com-right .com-btngroup{
    margin-top: 13px;
}
#mask .star-tc{
    margin-left: 31px;
    display: inline;
    width: 90px;
    height: 15px;
}
#mask #markstar{
    margin-left: 31px;
    display: inline-block;
    width: 90px;
    height: 15px;
}