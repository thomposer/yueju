/*******中间内容部分样式开始*******/
#container{
    width: 100%;
    height:568px;
    background-color: #eff3f5;
}
.wrapper{
    height:568px;
}
/*******中间内容部分样式结束*******/
/*************右侧message内容开始******************/
#message-content{
    width:770px;
    height: 524px;
    background-color: #fff;
    float: right;
    margin-top: 26px;
}
#message-ul{
    line-height: 20px;
    margin-top:30px;
    margin-left: 85px;
}
#message-ul li{
    width: 600px;
    height: 106px;
    margin-top: 18px;
    border-bottom: 1px solid #eff3f5;
    font-size: 12px;
    word-wrap: break-word;
    text-overflow: ellipsis;
    line-height: 25px;
    color: #767171;
}
/*#message-ul li p{*/
    /*line-height: 18px;*/
/*}*/
#message-ul li strong{
    font-size: 14px;
}
.message-time{
    margin-left: 34px;
}
/*************右侧message内容结束******************/

